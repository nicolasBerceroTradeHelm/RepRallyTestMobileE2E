{
  "name": "beverage-shop-e2e-tests",
  "version": "1.0.0",
  "description": "E2E tests for Beverage Shopping App",
  "main": "index.js",
  "scripts": {
    "test": "wdio run wdio.android.conf.js",
    "test:product-browsing": "wdio run wdio.android.conf.js --spec ./test/specs/android/01-product-browsing.test.js",
    "test:cart-management": "wdio run wdio.android.conf.js --spec ./test/specs/android/02-cart-management.test.js",
    "test:checkout": "wdio run wdio.android.conf.js --spec ./test/specs/android/03-checkout-flow.test.js",
    "test:all": "wdio run wdio.android.conf.js --spec ./test/specs/android/*.test.js"
  },
  "keywords": ["appium", "webdriverio", "e2e", "testing", "android"],
